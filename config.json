{
    "crawler": {
        "timeout": 30,
        "max_retries": 3,
        "user_agent": "DatasetCrawler/1.0",
        "follow_redirects": true
    },
    "output": {
        "format": "both",
        "compression": "snappy",
        "batch_size": 1000,
        "output_dir": "./output"
    },
    "urls": [
        "https://example.com",
        "https://example.org",
        "https://en.wikipedia.org/wiki/Machine_learning"
    ],
    "headers": {
        "Accept-Language": "en-US,en;q=0.9",
        "Accept-Encoding": "gzip, deflate",
        "Cache-Control": "no-cache"
    }
}
